function RenderEspMechUILIBRARY()
    local Visuals = Window:Page({Name = "Visuals"})
    local Visuals_Others, Visuals_Self = Visuals:MultiSection({Sections = {"Others" , "Self"}, Side = "Left", Size = 200})
    VisualsEnemies_BoxEsp = Visuals_Others:Toggle({Name = "Box", Default = false, Pointer = "VisualsEnemies_BoxEsp"})
    VisualsEnemies_BoxEsp:Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_BoxColor"})
    VisualsEnemies_NameEsp = Visuals_Others:Toggle({Name = "Name", Default = false, Pointer = "VisualsEnemies_NameEsp"})
    VisualsEnemies_NameEsp:Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_NameColor"})
    VisualsEnemies_TracerEsp = Visuals_Others:Toggle({Name = "Tracer", Default = false, Pointer = "VisualsEnemies_TracerEsp"})
    VisualsEnemies_TracerEsp :Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_TracerColor"})
    VisualsEnemies_HealthEsp = Visuals_Others:Toggle({Name = "Health", Default = false, Pointer = "VisualsEnemies_HPEsp"})
    VisualsEnemies_HealthEsp:Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_HealthColor"})
    VisualsEnemies_DistEsp = Visuals_Others:Toggle({Name = "Distance", Default = false, Pointer = "VisualsEnemies_DistEsp"})
    VisualsEnemies_DistEsp:Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_DistanceColor"})
    VisualsEnemies_ToolEsp = Visuals_Others:Toggle({Name = "Show Tool", Default = false, Pointer = "VisualsEnemies_ToolEsp"})
    VisualsEnemies_ToolEsp:Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_ToolColor"})
    VisualsEnemies_MoneyEsp = Visuals_Others:Toggle({Name = "Show Money", Default = false, Pointer = "VisualsEnemies_MoneyEsp"})
    VisualsEnemies_MoneyEsp:Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_MoneyColor"})
    VisualsEnemies_WantedEsp = Visuals_Others:Toggle({Name = "Show Wanted", Default = false, Pointer = "VisualsEnemies_WantedEsp"})
    VisualsEnemies_WantedEsp:Colorpicker({Info = "ESP-Box Color", Alpha = 0.75, Default = Color3.fromRGB(200, 200, 200), pointer = "VisualsEnemies_WantedColor"})
    VisualsEnemies_HealthBasedColors = Visuals_Others:Toggle({Name = "HP Based colors", Default = false, Pointer = "VISUALSHPBASED"})
end
