function RenderMiscMechUILIBRARY()
    local Misc = Window:Page({Name = "Misc"})
    local Misc_Main = Misc:Section({Name = "Character", Side = "Left"})
    local Checks_Misc = Misc:Section({Name = "Checks", Side = "Right"})
    local Other_Misc = Misc:Section({Name = "Teleports", Side = "Left"})
    
    Misc_Main:Toggle({Name = "Inf Stam", Default = false, Pointer = "Misc_MainSTam" , Callback = function() OnLocalCharacterAdded(game.Players.LocalPlayer.Character) end})
    Misc_Main:Toggle({Name = "No recoil", Default = false, Pointer = "Misc_MainNoRc"})
    Misc_Main:Toggle({Name = "No Fall damage", Default = false, Pointer = "Misc_MFD"})
    Misc_Main:Toggle({Name = "Jump Power", Default = false, Pointer = "Misc_MainJP", Callback = function() LocalPlayer.Character.Humanoid.JumpPower = OriginalJumpPower end})
    Misc_Main:Slider({Name = "Power", Minimum = 1, Maximum = 75, Default = 40, Decimals = 0.1, Pointer = "JPPOWERSLIDER"})
    Misc_Main:Toggle({Name = "Walk speed", Default = false, Pointer = "Misc_MainWS", Callback = function() LocalPlayer.Character.Humanoid.WalkSpeed = OriginalWalkspeed end})
    Misc_Main:Slider({Name = "Speed", Minimum = 1, Maximum = 30, Default = 15, Decimals = 0.1, Pointer = "WSSPEEDSLIDER"})
    
    Checks_Misc:Toggle({Name = "Aimbot non friends", Default = false, Pointer = "Checks_MiscFriends"})
    Checks_Misc:Toggle({Name = "Check Aimbot visibllity", Default = false, Pointer = "Checks_MiscVisible"})
    
    
end
--//
RenderAimbotMechUILIBRARY()
